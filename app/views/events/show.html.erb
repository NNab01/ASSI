<h1>Events#show</h1>
<div class="events">
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Date</th>
      <th>Location</th>
      <th>Left Presales</th>
      <th colspan="8"></th>
    </tr>
  </thead>
  <tbody>
  
  <tr>
    <td> <%= @event.title %></td> 
    <td><%= @event.price %></td>
    <td><%= @event.date %></td>
    <td><%= @event.location %></td>
    <td><%= @event.presales_left %></td>
    </tr>
   
  </tbody>
</table>
</div>
<div>
<%= link_to "Back",events_path %>
<%= link_to "Elimina",@event,data: {turbo_method: :delete}%> <!-- metodo base per riferirsi alla destroy altrimenti si hanno problemi -->

<%= link_to "Modifica",edit_event_path %>

</div>

